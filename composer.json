{
  "name": "ss-ipg/laravel-newrelic",
  "description": "Minimal NewRelic Functionality for Laravel",
  "keywords": [
    "laravel",
    "newrelic"
  ],
  "homepage": "https://github.com/ss-ipg/laravel-newrelic",
  "license": "MIT",
  "authors": [],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.4",
    "illuminate/contracts": "^10.0",
    "illuminate/support":  "^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0 || ^8.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "SecureSpace\\LaravelNewRelic\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SecureSpace\\LaravelNewRelic\\Tests\\": "tests"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ],
    "test": "vendor/bin/phpunit --configuration phpunit.xml"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "SecureSpace\\LaravelNewRelic\\LaravelNewRelicServiceProvider::class"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
